public class Armstrong {

	public static void main(String[] args) {
		int a=153;
		int rem, result=0;
		int b=a;
	

 while(a>0)
 {
	 rem=a%10;
	 result= (rem*rem*rem)+ result;
	 a= a/10;
	 b=a;
 }
 if(b==a)
 
	 System.out.println("It is an Armstrong number");
 else
	  System.out.println("It is not an Armstrong number");
	}

	}
